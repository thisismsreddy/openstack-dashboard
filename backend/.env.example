# OpenStack Keystone authentication endpoint (v3)
OS_AUTH_URL=https://your-openstack.example.com:5000/v3

# Admin credentials for service account
OS_ADMIN_USERNAME=admin
OS_ADMIN_PASSWORD=supersecret
OS_ADMIN_PROJECT_ID=00000000-0000-0000-0000-000000000000
OS_ADMIN_DOMAIN_ID=default
OS_DEFAULT_ROLE_NAME=_member_

# API endpoints for Compute (Nova) and Volume (Cinder)
# API endpoints for Compute (Nova), Volume (Cinder) and Network (Neutron)
OS_COMPUTE_URL=https://your-openstack.example.com:8774/v2.1
OS_VOLUME_URL=https://your-openstack.example.com:8776/v3
OS_NETWORK_URL=https://your-openstack.example.com:9696/v2.0

# JWT secrets and expiration settings
JWT_ACCESS_SECRET=your_jwt_access_secret
JWT_REFRESH_SECRET=your_jwt_refresh_secret
JWT_ACCESS_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d

# Server port (optional, default is 4000)
PORT=4000